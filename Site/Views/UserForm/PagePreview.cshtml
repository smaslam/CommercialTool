@{
    ViewBag.Title = "PagePreview";
    Layout = null;
}

<script src="~/Scripts/jquery-1.8.2.min.js"></script>
<script>
    $(document).ready(function () {
        $("#btnSubmit").click(function () {
            $(".droppedField").find(".control-label,input").each(function () {
                if ($(this).is('label')) {
                    alert($(this).html());
                }
                else if ($(this).is('input')) {
                    alert($(this).val());
                }
                else if ($(this).is("input[type='radio']").is(':checked')) {
                    //alert('adss');
                    alert($(this).parent().html());
                    //alert($(this).is("input[type='radio']:checked").val());
                }
            });

        });

    });


</script>
